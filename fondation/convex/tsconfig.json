{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "composite": false,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["*.ts"],
  "exclude": ["_generated/**/*", "*.js", "*.d.ts", "*.d.ts.map", "*.js.map"]
}